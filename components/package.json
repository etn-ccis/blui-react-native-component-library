{
    "name": "@brightlayer-ui/react-native-components",
    "version": "8.0.3",
    "author": "brightlayer-ui <brightlayer-ui@eaton.com>",
    "description": "Reusable React Native components for Brightlayer UI applications",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/etn-ccis/blui-react-native-component-library"
    },
    "keywords": [
        "react native",
        "components",
        "brightlayer-ui"
    ],
    "bugs": {
        "url": "https://github.com/etn-ccis/blui-react-native-component-library/issues"
    },
    "homepage": "https://github.com/etn-ccis/blui-react-native-component-library#readme",
    "license": "BSD-3-Clause",
    "main": "core/index.js",
    "types": "core/index.d.ts",
    "scripts": {
        "lint": "eslint \"src/**/*.{ts,tsx}\"",
        "lint:fix": "eslint \"src/**/*.{ts,tsx}\" --fix",
        "build": "tsc --p tsconfig.lib.json",
        "test": "jest",
        "test:watch": "jest --watch",
        "verify-build": "tsc --noEmit",
        "watch:showcase": "tsc --p tsconfig.lib.json --watch --outdir ../demos/showcase/node_modules/@brightlayer-ui/react-native-components",
        "prettier": "prettier \"src/**/**.{ts,tsx,js,jsx,json,css,scss,html}\" --write",
        "prettier:check": "prettier \"src/**/**.{ts,tsx,js,jsx,json,css,scss,html}\" --check"
    },
    "prettier": "@brightlayer-ui/prettier-config",
    "dependencies": {
        "@brightlayer-ui/react-native-themes": "7.0.0",
        "color": "^4.2.3",
        "react-native-collapsible": "^1.6.0",
        "react-native-keyboard-aware-scroll-view": "^0.9.5",
        "react-native-modal": ">=13.0.1",
        "react-native-reanimated": "^3.6.1"
    },
    "peerDependencies": {
        "@brightlayer-ui/react-native-vector-icons": "^2.0.0",
        "react": "^17.0.0 || ^18.0.0",
        "react-native": "0.68.0 || 0.69.0 || ^0.70.0",
        "react-native-paper": "^5.0.0",
        "react-native-safe-area-context": "^4.0.0",
        "react-native-vector-icons": "^9.0.0"
    },
    "devDependencies": {
        "@brightlayer-ui/eslint-config": "^3.0.1",
        "@brightlayer-ui/icons-svg": "^1.11.0",
        "@brightlayer-ui/prettier-config": "^1.0.3",
        "@brightlayer-ui/react-native-vector-icons": "^2.0.0",
        "@faker-js/faker": "^7.5.0",
        "@testing-library/react-native": "^12.1.2",
        "@types/color": "^3.0.3",
        "@types/jest": "^29.0.1",
        "@types/node": "^20.2.5",
        "@types/react-native-collapsible": "^0.11.0",
        "@types/react-native-vector-icons": "^6.4.12",
        "@types/react-test-renderer": "^18.0.0",
        "@typescript-eslint/eslint-plugin": "^5.36.2",
        "@typescript-eslint/parser": "5.62.0",
        "eslint": "^8.23.1",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-react": "^7.31.8",
        "identity-obj-proxy": "^3.0.0",
        "jest": "^29.7.0",
        "prettier": "^2.7.1",
        "react": "^18.1.0",
        "react-native": "^0.70.0",
        "react-native-paper": "^5.0.0",
        "react-native-safe-area-context": "^4.3.3",
        "react-native-svg": "^15.1.0",
        "react-native-vector-icons": "^9.2.0",
        "react-test-renderer": "^18.2.0",
        "ts-jest": "^29.0.0",
        "typescript": "4.9.5"
    },
    "files": [
        "package.json",
        "LICENSE",
        "README.md",
        "CHANGELOG.md",
        "core/"
    ],
    "directories": {
        "doc": "../docs"
    }
}
