        "react": "^16.11.0",
        "react-dom": "^16.11.0",
        "react-scripts": "3.2.0",
        "typescript": "^3.6.4"
    },
    "scripts": {
        "start": "export NODE_OPTIONS=--openssl-legacy-provider && yarn storybook",
        "storybook": "start-storybook -p 6006 -s ../../docs/,./assets",
        "build-storybook": "build-storybook -s ../../docs/,./assets",
        "lint": "eslint \"stories/**\"",
        "lint:fix": "eslint \"stories/**\" --fix",
        "prettier": "prettier \"**/**.{ts,tsx,js,jsx,json,css,scss,html}\" --write",
        "prettier:check": "prettier \"**/**.{ts,tsx,js,jsx,json,css,scss,html}\" --check"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "devDependencies": {
        "@babel/core": "^7.8.3",
        "@brightlayer-ui/eslint-config": "^1.1.5",
        "@brightlayer-ui/prettier-config": "^1.0.3",
