{
    "name": "storybook",
    "version": "0.1.0",
    "private": true,
    "prettier": "@brightlayer-ui/prettier-config",
    "dependencies": {
        "@brightlayer-ui/colors": "^3.0.1",
        "@brightlayer-ui/icons-mui": "^2.8.1",
        "@brightlayer-ui/react-native-components": "latest",
        "@brightlayer-ui/react-themes": "^6.1.0",
        "@brightlayer-ui/storybook-themes": "^1.0.4",
        "@material-ui/core": "^4.7.2",
        "@material-ui/icons": "^4.5.1",
        "@sambego/storybook-state": "^1.3.6",
        "@storybook/addon-notes": "^5.3.13",
        "@storybook/addons": "^5.3.13",
        "@storybook/preset-create-react-app": "^1.5.2",
        "@storybook/react": "^5.3.13",
